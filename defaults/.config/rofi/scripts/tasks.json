[
  {
    "name": "📦 arch update",
    "command": "$TERMINAL $HOME/.config/dotfiles/scripts/nix/arch.sh"
  },
  {
    "name": "🧹 arch clean",
    "command": "$TERMINAL $HOME/.config/dotfiles/scripts/nix/arch.sh -c"
  },
  {
    "name": "🔒 network-wireguard-wg0-home-start",
    "command": "sudo systemctl start systemd-resolved wg-quick@wg0"
  },
  {
    "name": "🔓 network-wireguard-wg0-home-stop",
    "command": "sudo systemctl stop wg-quick@wg0 systemd-resolved"
  },
  {
    "name": "☀️ flux dforce day",
    "command": "exec $HOME/.config/polybar/scripts/flux_/flux -v -c $HOME/.config/polybar/scripts/flux_/flux_config.lua -f day>>/tmp/flux.log 2>&1"
  },
  {
    "name": "🌑 flux force night",
    "command": "exec $HOME/.config/polybar/scripts/flux_/flux -v -c $HOME/.config/polybar/scripts/flux_/flux_config.lua -f night>>/tmp/flux.log 2>&1"
  },
  {
    "name": "🌑 flux force disable ",
    "command": "exec $HOME/.config/polybar/scripts/flux_/flux -v -c $HOME/.config/polybar/scripts/flux_/flux_config.lua -r >>/tmp/flux.log 2>&1"
  },
  {
    "name": "📷 qrencode clipboard",
    "command": "xclip -selection clipboard -o | qrencode -o - |  feh -FZ -"
  },
  {
    "name": "📎 polybar relaunch/restart",
    "command": "exec $HOME/.config/polybar/scripts/launch.sh"
  },
  {
    "name": "📎 dunst relaunch/restart",
    "command": "exec $HOME/.config/dunst/restart.sh"
  },
  {
    "name": "🎧 bluetooth wh-1000xm4",
    "command": "exec $HOME/.config/i3/scripts/bluetooth.lua 'connect' '14:3F:A6:EA:F5:D1'"
  },
  {
    "name": "🎧 bluetooth wh-1000xm2",
    "command": "exec $HOME/.config/i3/scripts/bluetooth.lua 'connect' '04:5D:4B:EA:9C:B2'"
  },
  {
    "name": "🎧 bluetooth philips pn402",
    "command": "exec $HOME/.config/i3/scripts/bluetooth.lua 'connect' '88:D0:39:48:7D:00'"
  },
  {
    "name": "💙 bluetooth rofi",
    "command": "rofi-bluetooth"
  },
  {
    "name": "📴 bluetooth disconnect",
    "command": "exec $HOME/.config/i3/scripts/bluetooth.lua 'disconnect'"
  },
  {
    "name": "🏖️ clearwater beach camera",
    "command": "$BROWSER https://www.visitstpeteclearwater.com/webcams"
  },
  {
    "name": "⚾ tampa bay rays livestream",
    "command": "python $HOME/Documents/scripts/selenium-chrome-buffstreams.py > /tmp/sel.log"
  },
  {
    "name": "📔 cheatsheet pdf",
    "command": "rofi -show file-browser-extended -file-browser-dir $HOME/Documents/Drive/wiki/cheatsheet"
  },
  {
    "name": "📔 cheatsheet navi",
    "command": "$TERMINAL --hold /usr/bin/navi --print"
  },
  {
    "name": "📔 cheatsheet cheat.sh",
    "command": "$TERMINAL /usr/bin/cht.sh --shell"
  },
  {
    "name": "📯 audio control",
    "command": "pavucontrol"
  },
  {
    "name": "🪟 screenshot full screen",
    "command": "scrot '/tmp/%Y-%m-%d_%T_scrot.png' -e 'xclip -selection clipboard -target image/png -i $f'"
  },
  {
    "name": "🪟 screenshot selection",
    "command": "scrot -fz -q 10 --line style=dash,width=3,color='red' -s '/tmp/%Y-%m-%d_%T_scrot.png' -e 'xclip -selection clipboard -target image/png -i $f'"
  },
  {
    "name": "🪟 screenshot window",
    "command": "scrot -u '/tmp/%Y-%m-%d_%T_scrot.png' -e 'xclip -selection clipboard -target image/png -i $f'"
  },
  {
    "name": "🪟 screenshot flameshot",
    "command": "flameshot gui"
  },
  {
    "name": "🎙️ audio select input",
    "command": "exec $HOME/.config/i3/scripts/pulseaudio-rofi.sh --input"
  },
  {
    "name": "📢 audio select output",
    "command": "exec $HOME/.config/i3/scripts/pulseaudio-rofi.sh --output"
  },
  {
    "name": "🖥️ power off",
    "command": "systemctl poweroff"
  },
  {
    "name": "💻 power reboot/restart",
    "command": "systemctl reboot"
  },
  {
    "name": "🔐 power lock",
    "command": "i3lock-fancy"
  },
  {
    "name": "🔑 pass root",
    "command": "pass --clip linux/`hostname`/user/root"
  },
  {
    "name": "🔑 pass skywafer",
    "command": "pass --clip nas/synology/skywafer/rei_login"
  },
  {
    "name": "🔑 pass ssh key github",
    "command": "pass --clip linux/`hostname`/ssh_key/id_ed25519_github"
  },
  {
    "name": "🔑 pass ssh key digitalocean",
    "command": "pass --clip linux/`hostname`/ssh_key/id_ed25519_digitalocean"
  },
  {
    "name": "🔑 pass ssh key gitlab",
    "command": "pass --clip linux/`hostname`/ssh_key/id_ed25519_gitlab"
  },
  {
    "name": "📂 htop cmus calc",
    "command": "$TERMINAL $HOME/.config/kitty/scratch.sh"
  },
  {
    "name": "📂 tickrs stonks market",
    "command": "$TERMINAL /usr/bin/tickrs"
  },
  {
    "name": "📂 whatsapp text",
    "command": "exec /usr/bin/firefox https://web.whatsapp.com https://messages.google.com/web"
  },
  {
    "name": "📧 email neomutt",
    "command": "$TERMINAL --hold $HOME/.config/kitty/neomutt.sh"
  },
  {
    "name": "🖼️ wallpaper internet random",
    "command": "exec ~/.config/polybar/scripts/get_wallpaper"
  },
  {
    "name": "🖼️ wallpaper arch random",
    "command": "exec feh --randomize --no-fehbg --bg-fill /usr/share/backgrounds/archlinux/*"
  },
  {
    "name": "🌃 redshift toggle night",
    "command": "exec ~/.config/polybar/scripts/redshift-toggle.sh"
  },
  {
    "name": "✍ compositor toggle",
    "command": "exec ~/.config/polybar/scripts/compton-toggle.sh"
  },
  {
    "name": "💻 xrandr main",
    "command": "exec $HOME/.config/i3/scripts/xrandr.sh main"
  },
  {
    "name": "📺 xrandr tv",
    "command": "exec $HOME/.config/i3/scripts/xrandr.sh tv"
  },
  {
    "name": "📺 xrandr home_dock",
    "command": "exec $HOME/.config/i3/scripts/xrandr.sh home_dock"
  },
  {
    "name": "📺 xrandr work_dock",
    "command": "exec $HOME/.config/i3/scripts/xrandr.sh work_dock"
  },
  {
    "name": "⌨️ xmodmap reload",
    "command": "xmodmap $HOME/.config/Xmodmap"
  },
  {
    "name": "⏳ pomodoro toggle",
    "command": "uairctl --socket /tmp/uair_pomo.socket toggle"
  },
  {
    "name": "⏳ pomodoro next",
    "command": "uairctl --socket /tmp/uair_pomo.socket next"
  },
  {
    "name": "⏳ pomodoro prev",
    "command": "uairctl --socket /tmp/uair_pomo.socket prev"
  },
  {
    "name": "⏳ pomodoro restart",
    "command": "pkill -x uair; echo '' > /tmp/uair_pomo.file; uair --socket /tmp/uair_pomo.socket > /tmp/uair_pomo.file&"
  },
  {
    "name": "xmodmap kinesis",
    "command": "xmodmap $HOME/.config/XmodmapKinesis"
  }
]
